DEFINE
    VALUE 1 n

    FUNCTION twice
        2 *
    END

    JOB job1 USE n
        n twice AS m
        m m * AS m
    RETURN m
END


{n 50 !=} REPEAT
    QUEUE job1
    n 1 + AS n
END

{DEPTH 0 !=} REPEAT
    RESULT
        m PRINT 
        GOTO lus
    END
    JOIN
    LABEL lus
    1 WAIT
END

"hello world " `char2prtbuff `printbuff

