DEFINE
    VALUE 1 n

    FUNCTION twice
        2 *
    END

    JOB job1 USE n
        n twice AS m
        m m * AS m
    RETURN m
END



FUNCTION hello
    "hello world " `char2prtbuff `printbuff
END



{n 5 >} DO
    QUEUE job1
    n 1 + AS n
END
n 50 == DO 
 QUEUE job1
END


{DEPTH 0 !=} DO
    RESULT
        m PRINT 
        GOTO lus
    END
    JOIN
    LABEL lus
    1 WAIT
END

hello



