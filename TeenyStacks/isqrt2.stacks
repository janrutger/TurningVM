1 AS s
{s 2000 !=} REPEAT
    TIMER 16 SET
    2 s < DO 
        1
        . PLOT
        GOTO einde
    END

    s 2 / AS x0
    s x0 / x0 + 2 / AS x1


    {x0 x1 <} REPEAT 
        x1 AS x0
        s x0 / x0 + 2 / AS x1
    END 

    x0 PLOT
    
    LABEL einde
    TIMER 16 PRINT
s 1 + AS s 
END