DEFINE
    ARRAY array [3 5 9]

    THING Ding 
        INIT
           1 [array] AS THIS n
           2 [array] AS THIS m
           3 AS m
        END

        THIS plus
            THIS m THIS n + PRINT
        END

        THIS minus
            THIS m THIS n -     
        END

        THIS multi
            THIS minus THIS minus * PRINT
        END

    END

END


5 6 *  m - PRINT

Ding plus
Ding multi
Ding minus PRINT


INPUT MATCH
        ON 1 DO
            1 PRINT
        END
        ON 2 DO
            2 PRINT
        END
        NO
            0 PRINT
        END
    END