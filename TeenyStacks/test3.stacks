DEFINE
    ARRAY array [3 5 9]

    THING Ding 
        INIT
           1 [array] AS THIS n
           2 [array] AS THIS m
           3 AS m
        END

        THIS plus
            THIS m THIS n + PRINT
        END

        THIS minus
            THIS m THIS n -     
        END
    END
END


5 6 * m - PRINT

Ding plus
Ding minus PRINT