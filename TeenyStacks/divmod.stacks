0 AS quotient
0 AS counter
. AS divisor 
. AS number

divisor 2 > GOTO uitzonderingen

{number counter <} REPEAT
    counter divisor + AS counter 
    quotient 1 + AS quotient
END

GOTO einde

LABEL uitzonderingen
    1 divisor == DO
        divisor AS quotient
        divisor AS counter
    END

    2 divisor == DO
    END

    0 divisor == DO
    END 

LABEL einde
    quotient counter number  -
