
LABEL loop
    {RAWIN 1 ==} REPEAT
        
        DUP "+" == DO
            DROP
            +
            GOTO done
            END
        DUP "-" == DO
            DROP
            - 
            GOTO done
            END
        DUP "*" == DO
            DROP
            * 
            GOTO done
            END
        DUP "/" == DO
            DROP
            / 
            GOTO done
            END
        DUP "." == DO
            DROP
            . PRINT 
            GOTO done
            END

        "q" == DO
            GOTO endloop
            END

    LABEL done
    END   
GOTO loop
LABEL endloop

