
LABEL loop
    {RAWIN 1 ==} REPEAT
        DUP "q" == DO
            POP
            GOTO endloop
            END
        DUP "+" == DO
            POP
            +
            GOTO done
            END
        DUP "-" == DO
            POP
            - 
            GOTO done
            END
        DUP "*" == DO
            POP
            * 
            GOTO done
            END
        DUP "/" == DO
            POP
            / 
            GOTO done
            END
        DUP "." == DO
            POP
            . PRINT 
            GOTO done
            END

    POP
    LABEL done
    END   
GOTO loop
LABEL endloop

